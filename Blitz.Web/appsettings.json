{
  "ConnectionStrings": {
    "BlitzPostgres": "Server=localhost;Database=blitz;UserId=blitz;Password=blitz;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning",
      "Microsoft.EntityFramework": "Warning"
    }
  },
  "GarbageCollector": {
    "Enabled": false,
    "Schedule": "*/6 * * * *",
    "MinAgeMinutes": 60,
    "MinKeptRecentExecutions": 20
  },
  "AllowedHosts": "*"
}
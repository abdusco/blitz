{
  "ConnectionStrings": {
    "BlitzPostgres": "Server=localhost;Database=blitz;UserId=blitz;Password=blitz;",
    "BlitzSqlServer": "Server=(localdb)\\blitz;Integrated Security=true;",
    "BlitzSqlite": "Data Source=blitz.sqlite3;Foreign Keys=true",
    "HangfireSqlite": "Data Source=hangfire.sqlite3;Foreign Keys=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning",
      "Microsoft.EntityFramework": "Information"
    }
  },
  "GarbageCollector": {
    "Enabled": false,
    "Schedule": "*/6 * * * *",
    "MinAgeMinutes": 60,
    "MinKeptRecentExecutions": 20
  },
  "AllowedHosts": "*"
}
